{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "YT": false,
        "Masonry": false
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-param-reassign": ["error", { "props": false }]
    }
}